{
    "name": "otsec/yii2-sentry",
    "description": "A Yii2 component for Sentry (http://getsentry.com)",
    "keywords": ["yii2", "sentry", "raven", "error", "crash"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Artem Belov",
            "email": "razor2909@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "sentry/sentry": "^1.6"
    },
    "suggest": {
        "bower-asset/raven-js": "to load javascript raven library not from CDN"
    },
    "autoload": {
        "psr-4" : {
            "otsec\\yii2\\sentry\\" : "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    }
}
